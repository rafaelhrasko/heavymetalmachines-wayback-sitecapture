//iife - immediately involked function expression , encapsulando
(function () {
  jQuery(document).ready(function () {
    function scrollNav() {
      jQuery("#navScroll a").click(function () {
        jQuery("#navScroll a.active").removeClass("active");
        jQuery(this).addClass("active");

        jQuery("html, body")
          .stop()
          .animate(
            {
              scrollTop: jQuery(jQuery(this).attr("href")).offset().top - 63,
            },
            300
          );
        return false;
      });
    }
    scrollNav();
  });

  // Debounce do Lodash - restringe quantidade de vezes que a função é chamada, ao scroll.
  debounce = function (func, wait, immediate) {
    var timeout;
    return function () {
      var context = this,
        args = arguments;
      var later = function () {
        timeout = null;
        if (!immediate) func.apply(context, args);
      };
      var callNow = immediate && !timeout;
      clearTimeout(timeout);
      timeout = setTimeout(later, wait);
      if (callNow) func.apply(context, args);
    };
  };

  // Cache selectors
  var lastId;
  var topMenu = jQuery("#navScroll");

  // All list items
  var menuItems = topMenu.find("a");

  // Anchors corresponding to menu items
  var scrollItems = menuItems.map(function () {
    var item = jQuery(jQuery(this).attr("href"));
    if (item.length) {
      return item;
    }
  });

  //função para adicionar o .active ao scrollar a página encapsulada por debounce
  jQuery(window).scroll(
    debounce(function () {
      highLightLinks();
    }, 100)
  );

  var highLightLinks = function () {
    // Get container scroll position
    var fromTop = jQuery(this).scrollTop();

    // Get id of current scroll item
    var cur = scrollItems.map(function () {
      if (jQuery(this).offset().top < fromTop + 64) return this;
    });

    // Get the id of the current element
    var cur = cur[cur.length - 1];
    var id = cur && cur.length ? cur[0].id : "";

    if (id.trim() == "") {
      id = "headePager";
    }

    if (lastId !== id) {
      lastId = id;
      // Set/remove active clas
      topMenu.find("a").removeClass("active");

      let myId = "a[href='#" + id + "']";
      topMenu.find(myId).addClass("active");
    }
  };
})();


